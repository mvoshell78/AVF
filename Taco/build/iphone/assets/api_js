var getJson=function(){xhr.open("GET","http://taco-randomizer.herokuapp.com/random/"),xhr.send()},remoteResponce=function(){var e=JSON.parse(this.responseText),r={shellName:e.shell.name,shellRecipe:e.shell.recipe,mixinName:e.mixin.name,mixinRecipe:e.mixin.recipe,mixinUrl:e.mixin.url,seasoningName:e.seasoning.name,seasoningRecipe:e.seasoning.recipe,seasoningUrl:e.seasoning.url,condimentName:e.condiment.name,condimentRecipe:e.condiment.recipe,condimentUrl:e.condiment.url,baseName:e.base_layer.name,baseRecipe:e.base_layer.recipe,baseUrl:e.base_layer.url},n=require("storage");n.storeData(r);var o=require("cloud");o.saveCloud(r)},remoteError=function(e){Ti.API.debug(status=this.status),Ti.API.debug(text=this.responceText),Ti.API.debug(error=e.error),alert("HTTP Client Unavailable")},xhr=Ti.Network.createHTTPClient({onload:remoteResponce,onError:remoteError,timeout:5e3});exports.remoteResponce=remoteResponce,exports.getJson=getJson;