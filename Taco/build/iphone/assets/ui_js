var storage=require("storage"),runUi=function(e){var o=Ti.UI.createWindow({backgroundImage:"/images/background.jpg",layout:"vertical"}),a=Ti.UI.createWindow({backgroundImage:"/images/background.jpg",layout:"vertical"}),i=(require("api"),Ti.UI.createScrollView({height:"80%",width:"50%",borderRadius:10,top:40,layout:"vertical",showVerticalScrollIndicator:!0,contentWidth:"auto",contentHeight:"auto",scrollType:"vertical"})),n=Ti.UI.createView({top:5,name:"baseName",height:"15%",width:"80%"}),t=Ti.UI.createView({top:5,name:"conName",height:"15%",width:"80%"}),r=Ti.UI.createView({top:5,name:"mixName",height:"15%",width:"80%"}),l=Ti.UI.createView({top:5,name:"seaName",height:"15%",width:"80%"}),c=Ti.UI.createView({top:5,name:"sheName",height:"15%",width:"80%"}),s=Ti.UI.createLabel({text:e.baseRecipe,font:{fontSize:20,fontFamily:"Roboto",fontWeight:"bold"},top:20,color:"black"}),m=Ti.UI.createLabel({text:e.baseRecipe,font:{fontSize:20,fontFamily:"Roboto"},top:20,color:"black"}),d=Ti.UI.createLabel({text:e.baseName,name:"baseName",font:{fontSize:20,fontFamily:"Roboto-thin"},top:20,color:"black"}),p=Ti.UI.createLabel({text:e.condimentName,name:"conName",font:{fontSize:20,fontFamily:"Roboto-thin"},top:20,color:"black"}),u=Ti.UI.createLabel({text:e.mixinName,name:"mixName",font:{fontSize:20,fontFamily:"Roboto-thin"},top:20,color:"black"}),N=Ti.UI.createLabel({text:e.seasoningName,name:"seaName",font:{fontSize:20,fontFamily:"Roboto-thin"},top:20,color:"black"}),g=Ti.UI.createLabel({text:e.shellName,name:"sheName",font:{fontSize:20,fontFamily:"Roboto-thin",fontWeight:"bold"},top:20,color:"black"}),T=Ti.UI.createView({height:"5%",top:40,width:"50%",name:"getNew",backgroundColor:"#BFCFCC"}),b=Ti.UI.createLabel({text:"Get new recipe",font:{fontSize:20,fontFamily:"Roboto"},name:"getNew",color:"black"}),R=Ti.UI.createLabel({text:"Take a pic",font:{fontSize:20,fontFamily:"Roboto"},name:"camera",color:"black"}),h=Ti.UI.createView({height:"5%",top:5,width:"50%",name:"camera",backgroundColor:"#BFCFCC"}),x=Ti.UI.createLabel({text:"Return to Recipes",font:{fontSize:20,fontFamily:"Roboto"},color:"black"}),f=Ti.UI.createView({backgroundColor:"#BFCFCC",height:"5%",width:"50%",bottom:0});f.add(x),T.add(b),o.add(T),o.add(n),o.add(t),o.add(r),o.add(l),o.add(c),i.add(s),i.add(m),a.add(i),a.add(f),n.add(d),t.add(p),r.add(u),l.add(N),c.add(g),h.add(R),o.add(h),o.open(),f.addEventListener("click",function(){console.log("back button fired");require("storage");a.close(),o.open()}),o.addEventListener("click",function(i){if(console.log("click"+i.source.name),"baseName"===i.source.name&&(console.log("base Name"),o.close(),s.text=e.baseName,m.text=e.condimentRecipe,a.open()),"conName"===i.source.name&&(console.log("con Name"),o.close(),s.text=e.condimentName,m.text=e.condimentRecipe,a.open()),"mixName"===i.source.name&&(console.log("mix Name"),o.open(),s.text=e.mixinName,m.text=e.mixinRecipe,a.open()),"seaName"===i.source.name&&(console.log("sea Name"),o.open(),s.text=e.seasoningName,m.text=e.seasoningRecipe,a.open()),"sheName"===i.source.name&&(console.log("she Name"),o.open(),s.text=e.shellName,m.text=e.shellRecipe,a.open()),"camera"===i.source.name&&(console.log("camera"),Ti.Media.showCamera({saveToPhotoGallery:!0,mediaTypes:[Ti.Media.MEDIA_TYPE_PHOTO],success:function(e){alert(e.mediaType===Ti.Media.MEDIA_TYPE_PHOTO?"Image saved to camera roll":"error"+e.mediaType)},error:function(e){alert(e.code===Ti.Media.NO_CAMERA?"No Camera Detected":"other error: "+e.code)}})),"getNew"===i.source.name){var n=require("api");n.getJson()}})};exports.runUi=runUi;