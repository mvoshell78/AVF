var api=require("api"),cloudModule=require("ti.cloud");cloudModule.debug=!0;var userId,loginCloud=function(){cloudModule.Users.login({login:"com.voshell.taco",password:"12345"},function(e){e.success?(console.log("cloud working!"),api.getJson()):alert("error "+e.message)})},saveCloud=function(e){console.log("cloud"),console.log(e),cloudModule.Objects.create({classname:"randomTacoRecipe",fields:{shellName:e.shellName,shellRecipe:e.shellRecipe,mixinName:e.mixinName,mixinRecipe:e.mixinRecipe,mixinUrl:e.mixinUrl,seasoningName:e.seasoningName,seasoningRecipe:e.seasoningRecipe,seasoningUrl:e.seasoningUrl,condimentName:e.condimentName,condimentRecipe:e.condimentRecipe,condimentUrl:e.condimentUrl,baseName:e.base_layerName,baseRecipe:e.base_layerRecipe,baseUrl:e.base_layerUrl}},function(e){e.success?console.log("Cloud Works!"):alert("Cloud no worky :(")})};exports.loginCloud=loginCloud,exports.saveCloud=saveCloud;