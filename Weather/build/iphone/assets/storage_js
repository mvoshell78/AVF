var readData=function(){for(var e=Ti.Database.open("weatherDB"),t=e.execute("SELECT * FROM weatherTBL"),o=t.rowCount;t.isValidRow();){var i={id:t.fieldByName("id"),time:t.fieldByName("time"),city:t.fieldByName("city"),temp:t.fieldByName("temp"),weat:t.fieldByName("weat"),hum:t.fieldByName("hum"),chill:t.fieldByName("chill"),dir:t.fieldByName("dir"),mph:t.fieldByName("mph"),gust:t.fieldByName("gust"),icon:t.fieldByName("icon")};t.next()}console.log(i),console.log("rowCount "+o),t.close(),e.close(),ui.changeTxt(i)},readData2=function(){for(var e=Ti.Database.open("weatherDB"),t=e.execute("SELECT * FROM weatherTBL2"),o=t.rowCount;t.isValidRow();){var i={title0:t.fieldByName("title0"),conditions0:t.fieldByName("conditions0"),icon0:t.fieldByName("icon0"),title1:t.fieldByName("title1"),conditions1:t.fieldByName("conditions1"),icon1:t.fieldByName("icon1"),title2:t.fieldByName("title2"),conditions2:t.fieldByName("conditions2"),icon2:t.fieldByName("icon2"),title3:t.fieldByName("title3"),conditions3:t.fieldByName("conditions3"),icon3:t.fieldByName("icon3"),title4:t.fieldByName("title4"),conditions4:t.fieldByName("conditions4"),icon4:t.fieldByName("icon4")};t.next()}console.log("rowCount "+o),t.close(),e.close(),ui.changeTxt2(i)},storeData=function(e){console.log("storeData ",e);var t=Ti.Database.open("weatherDB");t.execute("CREATE TABLE IF NOT EXISTS weatherTBL (id INTEGER PRIMARY KEY, time TEXT, city TEXT, temp TEXT, weat TEXT, hum TEXT, chill TEXT, dir TEXT, mph TEXT, gust TEXT, icon TEXT)"),t.execute("DELETE FROM weatherTBL"),t.execute("INSERT INTO weatherTBL(time, city, temp, weat, hum ,chill, dir, mph, gust, icon) VALUES (?,?,?,?,?,?,?,?,?,?)",e.time,e.city,e.temp,e.weat,e.hum,e.chill,e.dir,e.mph,e.gust,e.icon);var o=t.lastInsertRowId;console.log(o),t.close(),readData()},storeData2=function(e){console.log("storeData2 ",e);var t=Ti.Database.open("weatherDB");t.execute("CREATE TABLE IF NOT EXISTS weatherTBL2 (id INTEGER PRIMARY KEY, title0 TEXT, conditions0 TEXT, icon0 TEXT, title1 TEXT, conditions1 TEXT, icon1 TEXT, title2 TEXT, conditions2 TEXT, icon2 TEXT, title3 TEXT, conditions3 TEXT, icon3 TEXT, title4 TEXT, conditions4 TEXT, icon4 TEXT)"),t.execute("DELETE FROM weatherTBL2"),t.execute("INSERT INTO weatherTBL2(title0, conditions0, icon0, title1, conditions1, icon1, title2, conditions2, icon2, title3, conditions3, icon3, title4, conditions4, icon4) VALUES (?,?,?,?,?,?,?,?,?,?,?,?,?,?,?)",e.title0,e.conditions0,e.icon0,e.title1,e.conditions1,e.icon1,e.title2,e.conditions2,e.icon2,e.title3,e.conditions3,e.icon3,e.title4,e.conditions4,e.icon4);var o=t.lastInsertRowId;console.log(o),t.close(),readData2()};exports.storeData2=storeData2,exports.storeData=storeData,exports.readData2=readData2,exports.readData=readData;