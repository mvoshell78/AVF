var getGeo=function(){console.log("Geo is working!"),Ti.Geolocation.purpose="Weather would like to use your Geolocation",Ti.Geolocation.getCurrentPosition(function(e){if("android"===Ti.Platform.osname){console.log("These aren't the droids your looking for");var o=39.1,r=-75.4,i="http://api.wunderground.com/api/d0597510b3186fce/conditions/q/"+o+","+r+".json",a=require("weatherapi");a.createUrl(i)}else{console.log("An apple a day, keeps the doctor away!");var o=e.coords.latitude,r=e.coords.longitude,i="http://api.wunderground.com/api/d0597510b3186fce/conditions/q/"+o+","+r+".json",t="http://api.wunderground.com/api/d0597510b3186fce/forecast/q/"+o+","+r+".json",a=require("weatherapi");a.createUrl(i,t)}})};exports.getGeo=getGeo;